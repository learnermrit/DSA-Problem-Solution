/* Link list Node:

struct Node
{
    int data;
    struct Node* next;

    Node(int x){
        data = x;
        next = NULL;
    }
};

*/

// Deletes middle of linked list and returns head of the modified list
Node* deleteMid(Node* head)
{
    // Your Code Here
    if(!head) return head;
    
    Node* temp=head;
    Node* slow=head;
    temp=temp->next->next;
    
    while(temp!=NULL && temp->next!=NULL){
        temp=temp->next->next;
        slow=slow->next;
    }
    
    slow->next=slow->next->next;
    return head;
}
